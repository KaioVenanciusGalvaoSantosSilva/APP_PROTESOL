<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:animationCache="false"
    android:background="@color/paleta_principal"
    android:paddingLeft="4dp"
    android:paddingTop="4dp"
    android:paddingRight="4dp"
    android:paddingBottom="4dp"
    tools:context=".MainActivity">

    <com.google.android.material.chip.ChipGroup
        android:id="@+id/chipGroup"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="89dp"
        tools:layout_editor_absoluteY="200dp">

    </com.google.android.material.chip.ChipGroup>

    <RadioGroup
        android:id="@+id/radioGroup"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="250dp"
        tools:layout_editor_absoluteY="217dp" />

    <Button
        android:id="@+id/btnVoltarMain"
        android:layout_width="45dp"
        android:layout_height="35dp"
        android:background="@drawable/home24px"
        android:tint="#FFC107"
        app:backgroundTint="#FFC107"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.054"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.032" />

    <Button
        android:id="@+id/btnHistorico"
        android:layout_width="45dp"
        android:layout_height="35dp"
        android:background="@drawable/history24px"
        android:tint="#FFFFFF"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btnConfiguracoes"
        app:layout_constraintHorizontal_bias="0.481"
        app:layout_constraintStart_toEndOf="@+id/btnVoltarMain"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.037" />

    <Button
        android:id="@+id/btnConfiguracoes"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:background="@drawable/ic_settings_24px"
        android:tint="#FFFFFF"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.946"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.026" />


    <Button
        android:id="@+id/btnPassarProtetor"
        android:layout_width="246dp"
        android:layout_height="80dp"
        android:background="@drawable/redondo"
        android:fontFamily="@font/winkleregular"
        android:scaleY="1"
        android:text="@string/passarprotetor"

        android:textAllCaps="false"
        android:textColor="@color/paleta_principal"
        android:textSize="34sp"
        android:textStyle="normal"
        android:typeface="normal"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnHistorico"
        app:layout_constraintVertical_bias=".5" />

    <Button
        android:id="@+id/barra1"
        android:layout_width="66dp"
        android:layout_height="36dp"

        android:background="@drawable/redondo"
        android:fontFamily="@font/winkleregular"
        android:scaleY="1"
        android:textAllCaps="false"
        android:textColor="@color/paleta_principal"
        android:textSize="32sp"
        app:backgroundTint="#FFC107"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.18"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vezesAoDia"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/barra2"
        android:layout_width="66dp"
        android:layout_height="36dp"

        android:background="@drawable/redondo"
        android:fontFamily="@font/winkleregular"
        android:scaleY="1"
        android:textAllCaps="false"
        android:textColor="@color/paleta_principal"
        android:textSize="32sp"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/barra1"
        app:layout_constraintTop_toBottomOf="@+id/vezesAoDia"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/barra3"
        android:layout_width="66dp"
        android:layout_height="36dp"

        android:background="@drawable/redondo"
        android:fontFamily="@font/winkleregular"
        android:scaleY="1"
        android:textAllCaps="false"
        android:textColor="@color/paleta_principal"
        android:textSize="32sp"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/barra2"
        app:layout_constraintTop_toBottomOf="@+id/vezesAoDia"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/barra4"
        android:layout_width="66dp"
        android:layout_height="36dp"

        android:background="@drawable/redondo"
        android:fontFamily="@font/winkleregular"
        android:scaleY="1"
        android:textAllCaps="false"
        android:textColor="@color/paleta_principal"
        android:textSize="32sp"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/barra3"
        app:layout_constraintTop_toBottomOf="@+id/vezesAoDia"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="226dp"
        android:layout_height="179dp"
        app:layout_constraintBottom_toTopOf="@+id/btnPassarProtetor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnHistorico"
        app:srcCompat="@drawable/protsol" />

    <TextView
        android:id="@+id/proxVez"
        android:layout_width="149dp"
        android:layout_height="34dp"
        android:fontFamily="@font/seguisbi"
        android:gravity="center"
        android:text="@string/proximavez"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnPassarProtetor"
        app:layout_constraintVertical_bias="0.02" />

    <TextView
        android:id="@+id/vezesAoDia"
        android:layout_width="124dp"
        android:layout_height="31dp"
        android:fontFamily="@font/seguisbi"
        android:gravity="center"
        android:text="@string/vezesaodia"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cronometro"
        app:layout_constraintVertical_bias="0.053" />

    <TextView
        android:id="@+id/x1"
        android:layout_width="18dp"
        android:layout_height="19dp"
        android:fontFamily="@font/seguisbi"
        android:text="1X"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/barra1"
        app:layout_constraintHorizontal_bias="0.568"
        app:layout_constraintStart_toStartOf="@+id/barra1"
        app:layout_constraintTop_toBottomOf="@+id/barra1"
        app:layout_constraintVertical_bias="0.006" />

    <TextView
        android:id="@+id/x2"
        android:layout_width="17dp"
        android:layout_height="19dp"
        android:fontFamily="@font/seguisbi"
        android:text="2X"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="@+id/barra2"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toEndOf="@+id/barra1"
        app:layout_constraintTop_toBottomOf="@+id/barra2" />

    <TextView
        android:id="@+id/x3"
        android:layout_width="19dp"
        android:layout_height="19dp"
        android:fontFamily="@font/seguisbi"
        android:text="3X"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="@+id/barra3"
        app:layout_constraintHorizontal_bias="0.553"
        app:layout_constraintStart_toEndOf="@+id/barra2"
        app:layout_constraintTop_toBottomOf="@+id/barra3" />

    <TextView
        android:id="@+id/x4"
        android:layout_width="19dp"
        android:layout_height="19dp"
        android:fontFamily="@font/seguisbi"
        android:text="4X"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/barra4"
        app:layout_constraintHorizontal_bias="0.531"
        app:layout_constraintStart_toEndOf="@+id/barra3"
        app:layout_constraintTop_toBottomOf="@+id/barra4"
        app:layout_constraintVertical_bias="0.006" />

    <TextView
        android:id="@+id/cronometro"
        android:layout_width="170dp"
        android:layout_height="63dp"
        android:fontFamily="@font/seguisbi"
        android:gravity="center"
        android:text="02:00:00"
        android:textColor="#FFFFFF"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.55"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/proxVez"
        app:layout_constraintVertical_bias="0.0" />


    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="44dp"
        android:layout_height="46dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/cronometro"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/proxVez"
        app:layout_constraintVertical_bias="0.039"
        app:srcCompat="@drawable/accessalarm24px"
        app:tint="#FFFFFF" />


</androidx.constraintlayout.widget.ConstraintLayout>